{
  "name": "payment-api-server",
  "version": "1.0.0",
  "description": "Production-ready payment API server for MTN Mobile Money integration",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "payment": "node payment-api.js",
    "warm-cache": "node scripts/warm-cache.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed:negotiation": "node seed-negotiation-rules.js",
    "test:negotiation": "node test-negotiation.js",
    "aggregate:analytics": "node cron/negotiation-aggregator.js",
    "migrate:search": "node migrations/001_init_search_v2.js up",
    "migrate:rollback": "node migrations/001_init_search_v2.js down",
    "seed:search": "node scripts/seed-search-data.js",
    "index:products": "node -e \"require('./jobs/vectorIndexer').indexAll().then(() => process.exit())\"",
    "index:updated": "node -e \"require('./jobs/vectorIndexer').indexUpdated().then(() => process.exit())\"",
    "test:search": "node tests/searchV2.test.js"
  },
  "keywords": [
    "payment",
    "api",
    "mtn",
    "mobile-money",
    "express",
    "intouchpay"
  ],
  "author": "SEWITHDEBBY",
  "license": "MIT",
  "dependencies": {
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@tensorflow/tfjs": "^3.21.0",
    "@tensorflow/tfjs-backend-cpu": "^3.21.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^7.2.4",
    "cache-manager-fs-hash": "^3.0.0",
    "cloudinary": "^1.41.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.3",
    "helmet": "^8.1.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.5",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^7.0.9",
    "pg": "^8.16.3",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "sharp": "^0.33.5",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
